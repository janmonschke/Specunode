  .items
    float none
    display block
    margin 0 auto
  
    a
      padding .8em
      font-size 16px
      text-decoration none
      display block

      &:first-child
        background-color #E54661

      &:nth-child(2)
        background-color #FFA644

      &:nth-child(3)
        background-color #998A2F

      &:nth-child(4)
        background-color #2C594F